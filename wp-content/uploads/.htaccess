
# BEGIN WebP Converter
# ! --- DO NOT EDIT PREVIOUS LINE --- !
<IfModule mod_rewrite.c>
  RewriteEngine On
  RewriteCond %{HTTP_ACCEPT} image/webp
  RewriteCond %{DOCUMENT_ROOT}/htclawfirm.vn/wp-content/uploads-webpc/uploads/$1.jpg.webp -f
  RewriteCond %{HTTP_HOST}@@%{HTTP_REFERER} ^([^@]*)@@https?://\1/.*
  RewriteRule (.+)\.jpg$ /htclawfirm.vn/wp-content/uploads-webpc/uploads/$1.jpg.webp [NC,T=image/webp,E=cache-control:no-cache,L]
  RewriteCond %{HTTP_ACCEPT} image/webp
  RewriteCond %{DOCUMENT_ROOT}/htclawfirm.vn/wp-content/uploads-webpc/uploads/$1.jpeg.webp -f
  RewriteCond %{HTTP_HOST}@@%{HTTP_REFERER} ^([^@]*)@@https?://\1/.*
  RewriteRule (.+)\.jpeg$ /htclawfirm.vn/wp-content/uploads-webpc/uploads/$1.jpeg.webp [NC,T=image/webp,E=cache-control:no-cache,L]
  RewriteCond %{HTTP_ACCEPT} image/webp
  RewriteCond %{DOCUMENT_ROOT}/htclawfirm.vn/wp-content/uploads-webpc/uploads/$1.png.webp -f
  RewriteCond %{HTTP_HOST}@@%{HTTP_REFERER} ^([^@]*)@@https?://\1/.*
  RewriteRule (.+)\.png$ /htclawfirm.vn/wp-content/uploads-webpc/uploads/$1.png.webp [NC,T=image/webp,E=cache-control:no-cache,L]
</IfModule>
# ! --- DO NOT EDIT NEXT LINE --- !
# END WebP Converter

